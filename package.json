{
  "name": "@hankgalt/comff-profiles-tsclient",
  "version": "0.0.21",
  "description": "Comfforts profiles service TS client",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp -r ./lib/proto ./dist/lib/proto",
    "build:types": "tsc -d --emitDeclarationOnly",
    "build:proto": "sh ./scripts/build-proto.sh ",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --collectCoverage --detectOpenHandles",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comfforts/comff-profiles-TSClient.git"
  },
  "keywords": [
    "Comfforts",
    "Profiles",
    "Service",
    "Client",
    "Typescript"
  ],
  "author": "vtalwar.w@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/comfforts/comff-profiles-TSClient/issues"
  },
  "homepage": "https://github.com/comfforts/comff-profiles-TSClient#readme",
  "dependencies": {
    "@grpc/grpc-js": "1.8.14"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@hankgalt/comff-geo-tsclient": "0.0.23",
    "@jest/globals": "^29.6.2",
    "@types/jest": "29.5.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "babel-jest": "^29.7.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "grpc-tools": "1.12.4",
    "grpc_tools_node_protoc_ts": "5.3.3",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "directories": {
    "lib": "lib"
  }
}
